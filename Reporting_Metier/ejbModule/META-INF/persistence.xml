<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="Reporting_Metier">
	<jta-data-source>java:/postgresDS</jta-data-source>
	<class>com.reporting.metier.entities.ChartDynamic</class>
	<class>com.reporting.metier.entities.Datagrid</class>
	<class>com.reporting.metier.entities.Noeud</class>
	<class>com.reporting.metier.entities.PlanTarifaire</class>
	<class>com.reporting.metier.entities.Report</class>
	<class>com.reporting.metier.entities.StatMsc</class>
	<class>com.reporting.metier.entities.TypeDestination</class>
	<properties>
		<property name="hibernate.hbm2ddl.auto" value="update"/>
		<property name="hibernate.show_sql" value="true"/>
		<property name="hibernate.format_sql" value="true"/>
		
		</properties>
	</persistence-unit>
</persistence>
